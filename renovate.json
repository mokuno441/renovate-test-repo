{
  "platform": "github",
  "dependencyDashboard": true,
  "regexManagers": [
    {
      "fileMatch": ["^versions.ini"],
      "matchStrings": ["VERSION=(?<currentValue>.*)"],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "mokuno441/renovate-test-repo",
      "versioningTemplate": "semver-coerced"
    }
  ]
}